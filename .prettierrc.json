{
    "trailingComma": "es5",
    "tabWidth": 4,
    "semi": true,
    "singleQuote": true,
    "printWidth": 80,
    "bracketSameLine": true,
    "importOrder": [
        "(index.scss)$",
        "react",
        "formik",
        "yup",
        "i18next",
        "i18next-http-backend",
        "axios",
        "^@mui/(.*)$",
        "classnames",
        "^core$",
        "^(.*?)/interfaces/(.*)$",
        "^(.*?)/interfaces$",
        "^(.*?)/enums/(.*)$",
        "^(.*?)/enums$",
        "^(.*?)/utils/(.*)$",
        "^(.*?)/utils$",
        "^(.*?)/constants/(.*)$",
        "^constants/(.*)$",
        "^(.*?)/hooks/(.*)$",
        "^(.*?)/hooks(.*)$",
        "^(.*?)/hooks",
        "^core/translations/hooks$",
        "^(.*?)/components/(.*)$",
        "^components/(.*)$",
        "<THIRD_PARTY_MODULES>",
        "^(.*?)services/(.*)$",
        "^(.*?)context/(.*)$",
        "^(.*?)/assets/(.*)$",
        "^assets/(.*)$",
        "(.module.scss)$"
    ],
    "importOrderSeparation": false,
    "importOrderSortSpecifiers": true
}
