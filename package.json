{
  "name": "my-webpack-project",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "webpack --mode=production --node-env=production",
    "start": "next start",
    "lint": "next lint",
    "codegen": "graphql-codegen --config ../next-q-task/codegen.ts",
    "generate": "graphql-codegen --config ../next-q-task/codegen.ts",
    "storybook": "start-storybook -p 6006 ./public",
    "build-storybook": "build-storybook public",
    "build:dev": "webpack --mode=development",
    "build:prod": "webpack --mode=production --node-env=production",
    "watch": "webpack --watch",
    "test": "jest --watch",
    "coverage": "jest --coverage"
  },
  "jest": {
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
      "\\.(css|less|scss|sass)$": "identity-obj-proxy"
    },
    "clearMocks": true,
    "testEnvironment": "jsdom"
  },
  "dependencies": {
    "@apollo/client": "^3.7.0",
    "@babel/preset-typescript": "^7.18.6",
    "@storybook/preset-scss": "^1.0.3",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^14.0.0",
    "babel-core": "^6.26.3",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "graphql": "^16.6.0",
    "next": "12.3.1",
    "prettier": "^2.7.1",
    "react": "^18.2.0",
    "react-dom": "18.2.0",
    "react-spinner": "^0.2.7",
    "react-spinners": "^0.13.4",
    "sass": "^1.55.0",
    "ts-jest": "^29.0.3",
    "type-graphql": "^1.1.1",
    "webpack": "^5.74.0"
  },
  "devDependencies": {
    "@babel/core": "^7.19.3",
    "@graphql-codegen/cli": "2.13.5",
    "@graphql-codegen/client-preset": "1.0.5",
    "@graphql-codegen/near-operation-file-preset": "^2.4.2",
    "@graphql-codegen/typescript-react-apollo": "^3.3.4",
    "@storybook/addon-actions": "^6.5.12",
    "@storybook/addon-essentials": "^6.5.12",
    "@storybook/addon-interactions": "^6.5.12",
    "@storybook/addon-links": "^6.5.12",
    "@storybook/builder-webpack5": "^6.5.12",
    "@storybook/manager-webpack5": "^6.5.12",
    "@storybook/react": "^6.5.12",
    "@storybook/testing-library": "^0.0.13",
    "@types/node": "18.7.23",
    "@types/react": "^18.0.21",
    "@types/react-dom": "18.0.6",
    "@typescript-eslint/eslint-plugin": "^5.40.0",
    "@typescript-eslint/parser": "^5.40.0",
    "@webpack-cli/generators": "^2.5.0",
    "babel-loader": "^8.2.5",
    "css-loader": "^6.7.1",
    "eslint": "8.7.0",
    "eslint-config-next": "12.0.8",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-react": "^7.28.0",
    "html-webpack-plugin": "^5.5.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.2.1",
    "jest-environment-jsdom": "^29.2.1",
    "sass-loader": "^13.1.0",
    "style-loader": "^3.3.1",
    "typescript": "^4.8.4",
    "webpack-cli": "^4.10.0"
  },
